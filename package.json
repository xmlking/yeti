{
  "name": "yeti",
  "version": "0.0.0",
  "license": "MIT",
  "workspaces": [
    "libs/*",
    "apps/*",
    "tools"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "release": "standard-version",
    "scully": "scully",
    "scully:serve": "scully serve",
    "scully:all0": "rm -rf dist && yarn run build --prod && yarn run scully && yarn run scully serve",
    "scully:all": "rm -rf dist && yarn run build --prod && yarn run scully && yarn run lite-server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.0-rc.7",
    "@angular/cdk": "^8.0.0",
    "@angular/common": "9.0.0-rc.7",
    "@angular/compiler": "9.0.0-rc.7",
    "@angular/core": "9.0.0-rc.7",
    "@angular/forms": "9.0.0-rc.7",
    "@angular/localize": "^9.0.0-rc.7",
    "@angular/platform-browser": "9.0.0-rc.7",
    "@angular/platform-browser-dynamic": "9.0.0-rc.7",
    "@angular/router": "9.0.0-rc.7",
    "@angular/service-worker": "9.0.0-rc.7",
    "@nebular/auth": "^5.0.0-beta.1",
    "@nebular/eva-icons": "5.0.0-beta.1",
    "@nebular/security": "^5.0.0-beta.1",
    "@nebular/theme": "^5.0.0-beta.1",
    "@ngneat/until-destroy": "^7.1.0",
    "@ngxs/devtools-plugin": "^3.6.0",
    "@ngxs/form-plugin": "^3.6.0",
    "@ngxs/logger-plugin": "^3.6.0",
    "@ngxs/router-plugin": "^3.6.0",
    "@ngxs/storage-plugin": "^3.6.0",
    "@ngxs/store": "^3.6.0",
    "@scullyio/ng-lib": "latest",
    "eva-icons": "^1.1.2",
    "ngx-auto-unsubscribe": "3.0.1",
    "prismjs": "^1.17.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.4",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.0-rc.7",
    "@angular/cli": "9.0.0-rc.7",
    "@angular/compiler-cli": "9.0.0-rc.7",
    "@angular/language-service": "9.0.0-rc.7",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@nrwl/angular": "9.0.0-beta.3",
    "@nrwl/cypress": "9.0.0-beta.3",
    "@nrwl/jest": "9.0.0-beta.3",
    "@nrwl/nest": "9.0.0-beta.3",
    "@nrwl/node": "9.0.0-beta.3",
    "@nrwl/workspace": "9.0.0-beta.3",
    "@schematics/angular": "9.0.0-rc.7",
    "@scullyio/init": "^0.0.9",
    "@scullyio/scully": "latest",
    "@types/jest": "24.0.24",
    "@types/node": "13.1.0",
    "angular-cli-ghpages": "^0.6.1",
    "codelyzer": "5.2.1",
    "commitizen": "4.0.3",
    "cypress": "3.8.0",
    "cz-conventional-changelog": "3.0.2",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "jest-preset-angular": "7.1.1",
    "lite-server": "^2.5.4",
    "ngx-semantic-version": "2.0.0-beta.3",
    "prettier": "1.19.1",
    "standard-version": "7.0.1",
    "ts-jest": "24.2.0",
    "ts-node": "8.5.4",
    "tslint": "5.20.1",
    "typescript": "3.6.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}

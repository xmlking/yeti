{
  "name": "yeti",
  "version": "0.1.3",
  "license": "MIT",
  "workspaces": [
    "libs/*",
    "apps/*",
    "tools"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "// - BUILDING": "Build Apps for Prod, Mock environments",
    "prebuild": "rimraf dist coverage",
    "build:prod": "ng build webapp -c=production",
    "deploy:mock": "ng deploy --base-href=/ngx-starter-kit/ --configuration=mock",
    "// - ANALYTICS": "Analyze builds",
    "report-coverage": "codecov --token=$CODECOV_TOKEN",
    "// - RELEASE": "release Apps and Libs",
    "release": "standard-version",
    "// - DOCS": "Generate and serve docs",
    "doc:build": "compodoc -p tsconfig.json -d dist/docs",
    "doc:serve": "compodoc -s -d dist/docs",
    "doc:build-serve": "compodoc -p tsconfig.json -d dist/docs -s",
    "// - AFFECTED": "work on only affected modules",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:deploy": "nx affected:deploy",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "// - UNIVERSAL": "angular universal commands",
    "dev:ssr": "ng run webapp:serve-ssr",
    "serve:ssr": "node dist/apps/webapp/server/main.js",
    "build:ssr": "ng build --prod && ng run webapp:server:production",
    "prerender": "ng run webapp:prerender",
    "// - EXTRA": "Extra commands",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "check:demo": "node dist/tools/scripts/demo.js",
    "gen:metadata": "node tools/scripts/metagen.mjs",
    "build:tools": "rimraf dist/tools && tsc -p ./tools/tsconfig.tools.json",
    "run:tools": "ts-node --project ./tools/tsconfig.tools.json -r tsconfig-paths/register",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  },
  "lint-staged": {
    "{apps,libs}/**/*.{ts,json,md,scss,html}": [
      "yarn affected:lint --uncommitted --parallel --fix=true",
      "yarn format:write --uncommitted"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.1.1",
    "@angular/cdk": "12.1.1",
    "@angular/common": "12.1.1",
    "@angular/compiler": "12.1.1",
    "@angular/core": "12.1.1",
    "@angular/fire": "6.1.5",
    "@angular/flex-layout": "12.0.0-beta.34",
    "@angular/forms": "12.1.1",
    "@angular/localize": "12.1.1",
    "@angular/platform-browser": "12.1.1",
    "@angular/platform-browser-dynamic": "12.1.1",
    "@angular/platform-server": "12.1.1",
    "@angular/router": "12.1.1",
    "@angular/service-worker": "12.1.1",
    "@nebular/auth": "8.0.0",
    "@nebular/eva-icons": "8.0.0",
    "@nebular/security": "8.0.0",
    "@nebular/theme": "8.0.0",
    "@ngneat/until-destroy": "8.1.0",
    "@nguniversal/express-engine": "12.1.0",
    "@ngxs-labs/select-snapshot": "3.0.0",
    "@ngxs/devtools-plugin": "3.7.2",
    "@ngxs/form-plugin": "3.7.2",
    "@ngxs/logger-plugin": "3.7.2",
    "@ngxs/router-plugin": "3.7.2",
    "@ngxs/storage-plugin": "3.7.2",
    "@ngxs/store": "3.7.2",
    "@okta/okta-angular": "^3.1.0",
    "@rx-angular/template": "1.0.0-beta.25",
    "eva-icons": "1.1.3",
    "express": "4.17.1",
    "firebase": "8.7.0",
    "google-protobuf": "3.17.3",
    "grpc-web": "1.2.1",
    "ngx-auto-unsubscribe": "3.0.1",
    "ngx-markdown": "12.0.1",
    "prismjs": "1.24.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.1.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1201.1",
    "@angular-devkit/build-angular": "12.1.1",
    "@angular-eslint/eslint-plugin": "12.2.0",
    "@angular-eslint/eslint-plugin-template": "12.2.0",
    "@angular-eslint/template-parser": "12.2.0",
    "@angular/cli": "12.1.1",
    "@angular/compiler-cli": "12.1.1",
    "@angular/language-service": "12.1.1",
    "@compodoc/compodoc": "1.1.11",
    "@fortawesome/fontawesome-free": "5.15.3",
    "@ngneat/tailwind": "^7.0.3",
    "@nguniversal/builders": "12.1.0",
    "@ngx-builders/analyze": "2.3.0",
    "@nrwl/angular": "12.5.4",
    "@nrwl/cli": "12.5.4",
    "@nrwl/cypress": "12.5.4",
    "@nrwl/eslint-plugin-nx": "12.5.4",
    "@nrwl/jest": "12.5.4",
    "@nrwl/linter": "12.5.4",
    "@nrwl/nest": "12.5.4",
    "@nrwl/node": "12.5.4",
    "@nrwl/nx-cloud": "12.2.7",
    "@nrwl/workspace": "12.5.4",
    "@nx-go/nx-go": "1.7.1",
    "@schematics/angular": "12.1.1",
    "@twittwer/compodoc": "1.5.3",
    "@types/express": "4.17.12",
    "@types/google-protobuf": "3.15.2",
    "@types/jest": "26.0.23",
    "@types/node": "16.0.0",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "autoprefixer": "^10.2.6",
    "codecov": "3.8.2",
    "commitizen": "4.2.4",
    "cypress": "7.6.0",
    "dotenv": "10.0.0",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsdoc": "35.4.1",
    "eslint-plugin-prefer-arrow": "latest",
    "jest": "27.0.6",
    "jest-preset-angular": "9.0.4",
    "postcss": "^8.3.5",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "standard-version": "9.3.0",
    "tailwindcss": "^2.2.4",
    "ts-jest": "27.0.3",
    "ts-node": "10.0.0",
    "typescript": "4.3.5"
  },
  "optionalDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "cz-conventional-changelog": "3.3.0",
    "firebase-tools": "9.14.0",
    "fuzzy": "0.1.3",
    "grpc-tools": "^1.11.2",
    "grpc_tools_node_protoc_ts": "^5.3.0",
    "husky": "7.0.0",
    "inquirer": "8.1.1",
    "inquirer-autocomplete-prompt": "1.4.0",
    "lint-staged": "11.0.0",
    "lite-server": "2.6.1",
    "ngx-semantic-version": "2.3.0",
    "parse-md": "^2.0.4",
    "ts-proto": "1.82.0"
  }
}

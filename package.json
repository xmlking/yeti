{
  "name": "yeti",
  "version": "0.1.2",
  "license": "MIT",
  "workspaces": [
    "libs/*",
    "apps/*",
    "tools"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "// - BUILDING": "Build Apps for Prod, Mock environments",
    "prebuild": "rimraf dist coverage",
    "build:prod": "ng build webapp -c=production",
    "deploy:mock": "ng deploy --base-href=/ngx-starter-kit/ --configuration=mock",
    "// - ANALYTICS": "Analyze builds",
    "bundle-report": "ng build -c=production --stats-json && webpack-bundle-analyzer dist/apps/webapp/stats-es2015.json",
    "report-coverage": "codecov --token=$CODECOV_TOKEN",
    "// - RELEASE": "release Apps and Libs",
    "release": "standard-version",
    "// - DOCS": "Generate and serve docs",
    "doc:build": "compodoc -p tsconfig.json -d dist/docs",
    "doc:serve": "compodoc -s -d dist/docs",
    "doc:build-serve": "compodoc -p tsconfig.json -d dist/docs -s",
    "// - AFFECTED": "work on only affected modules",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:deploy": "nx affected:deploy",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "migrate": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "// - UNIVERSAL": "angular universal commands",
    "dev:ssr": "ng run webapp:serve-ssr",
    "serve:ssr": "node dist/apps/webapp/server/main.js",
    "build:ssr": "ng build --prod && ng run webapp:server:production",
    "prerender": "ng run webapp:prerender",
    "// - EXTRA": "Extra commands",
    "check:demo": "node dist/tools/scripts/demo.js",
    "gen:metadata": "node tools/scripts/metagen.mjs",
    "build:tools": "rimraf dist/tools && tsc -p ./tools/tsconfig.tools.json",
    "run:tools": "ts-node --project ./tools/tsconfig.tools.json -r tsconfig-paths/register"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  },
  "lint-staged": {
    "{apps,libs}/**/*.{ts,json,md,scss,html}": [
      "yarn affected:lint --uncommitted --parallel --fix=true",
      "yarn format:write --uncommitted"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.7",
    "@angular/cdk": "9.1.3",
    "@angular/common": "9.0.7",
    "@angular/compiler": "9.0.7",
    "@angular/core": "9.0.7",
    "@angular/fire": "6.0.0-rc.1",
    "@angular/flex-layout": "9.0.0-beta.29",
    "@angular/forms": "9.0.7",
    "@angular/localize": "9.0.7",
    "@angular/platform-browser": "9.0.7",
    "@angular/platform-browser-dynamic": "9.0.7",
    "@angular/platform-server": "9.0.7",
    "@angular/router": "9.0.7",
    "@angular/service-worker": "9.0.7",
    "@nebular/auth": "5.0.0",
    "@nebular/eva-icons": "5.0.0",
    "@nebular/security": "5.0.0",
    "@nebular/theme": "5.0.0",
    "@ngneat/until-destroy": "7.1.1",
    "@nguniversal/express-engine": "9.0.2",
    "@ngxs-labs/select-snapshot": "1.1.0",
    "@ngxs/devtools-plugin": "3.6.2",
    "@ngxs/form-plugin": "3.6.2",
    "@ngxs/logger-plugin": "3.6.2",
    "@ngxs/router-plugin": "3.6.2",
    "@ngxs/storage-plugin": "3.6.2",
    "@ngxs/store": "3.6.2",
    "eva-icons": "1.1.3",
    "express": "4.17.1",
    "firebase": "7.12.0",
    "google-protobuf": "3.11.4",
    "grpc-web": "1.0.7",
    "ngx-auto-unsubscribe": "3.0.1",
    "ngx-markdown": "9.0.0",
    "prismjs": "1.19.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.4",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.900.7",
    "@angular-devkit/build-angular": "0.900.7",
    "@angular/cli": "9.0.7",
    "@angular/compiler-cli": "9.0.7",
    "@angular/language-service": "9.0.7",
    "@compodoc/compodoc": "1.1.11",
    "@fortawesome/fontawesome-free": "5.13.0",
    "@nguniversal/builders": "9.0.2",
    "@nrwl/angular": "9.1.2",
    "@nrwl/cypress": "9.1.2",
    "@nrwl/jest": "9.1.2",
    "@nrwl/nest": "9.1.2",
    "@nrwl/node": "9.1.2",
    "@nrwl/workspace": "9.1.2",
    "@schematics/angular": "9.0.7",
    "@types/express": "4.17.3",
    "@types/google-protobuf": "3.7.2",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.3",
    "codecov": "3.6.5",
    "codelyzer": "5.2.1",
    "commitizen": "4.0.3",
    "cypress": "4.2.0",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "jest": "25.1.0",
    "jest-preset-angular": "8.1.2",
    "prettier": "2.0.2",
    "rimraf": "3.0.2",
    "standard-version": "7.1.0",
    "ts-jest": "25.2.1",
    "ts-node": "8.8.1",
    "tslint": "6.1.0",
    "typescript": "3.8.3"
  },
  "optionalDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "cz-conventional-changelog": "3.1.0",
    "firebase-tools": "7.16.1",
    "fuzzy": "0.1.3",
    "husky": "4.2.3",
    "inquirer": "7.1.0",
    "inquirer-autocomplete-prompt": "1.0.2",
    "lint-staged": "10.0.9",
    "lite-server": "2.5.4",
    "ngx-semantic-version": "2.0.0-beta.5",
    "parse-md": "^2.0.4",
    "ts-proto": "1.14.0",
    "webpack-bundle-analyzer": "3.6.1"
  }
}

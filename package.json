{
  "name": "yeti",
  "version": "0.0.0",
  "license": "MIT",
  "workspaces": [
    "libs/*",
    "apps/*",
    "tools"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "// - BUILDING": "Build Apps for Prod, Mock environments",
    "prebuild": "rimraf dist coverage",
    "build:prod": "ng build webapp -c=production",
    "deploy:mock": "ng deploy --base-href=/ngx-starter-kit/ --configuration=mock",
    "// - ANALYTICS": "Analyze builds",
    "bundle-report": "ng build -c=production --stats-json && webpack-bundle-analyzer dist/apps/webapp/stats-es2015.json",
    "report-coverage": "codecov --token=$CODECOV_TOKEN",
    "// - RELEASE": "release Apps and Libs",
    "release": "standard-version",
    "// - DOCS": "Generate and serve docs",
    "doc:build": "compodoc -p tsconfig.json -d dist/docs",
    "doc:serve": "compodoc -s -d dist/docs",
    "doc:build-serve": "compodoc -p tsconfig.json -d dist/docs -s",
    "// - SCULLY": "SSR, Blog processing",
    "scully": "scully",
    "scully:serve": "scully serve",
    "scully:all0": "rm -rf dist && yarn run build --prod && yarn run scully && yarn run scully serve",
    "scully:all": "rm -rf dist && yarn run build --prod && yarn run scully && yarn run lite-server",
    "// - AFFECTED": "work on only affected modules",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:deploy": "nx affected:deploy",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "{apps,libs}/**/*.{ts,json,md,scss,html}": [
      "yarn affected:lint --uncommitted --parallel --fix=true",
      "yarn format:write --uncommitted"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.0-rc.11",
    "@angular/cdk": "9.0.0-rc.8",
    "@angular/common": "9.0.0-rc.11",
    "@angular/compiler": "9.0.0-rc.11",
    "@angular/core": "9.0.0-rc.11",
    "@angular/fire": "5.3.0",
    "@angular/forms": "9.0.0-rc.11",
    "@angular/localize": "9.0.0-rc.11",
    "@angular/platform-browser": "9.0.0-rc.11",
    "@angular/platform-browser-dynamic": "9.0.0-rc.11",
    "@angular/router": "9.0.0-rc.11",
    "@angular/service-worker": "9.0.0-rc.11",
    "@nebular/auth": "5.0.0-beta.2",
    "@nebular/eva-icons": "5.0.0-beta.2",
    "@nebular/security": "5.0.0-beta.2",
    "@nebular/theme": "5.0.0-beta.2",
    "@ngneat/until-destroy": "7.1.0",
    "@ngxs-labs/select-snapshot": "1.1.0",
    "@ngxs/devtools-plugin": "3.6.1",
    "@ngxs/form-plugin": "3.6.1",
    "@ngxs/logger-plugin": "3.6.1",
    "@ngxs/router-plugin": "3.6.1",
    "@ngxs/storage-plugin": "3.6.1",
    "@ngxs/store": "3.6.1",
    "@scullyio/ng-lib": "0.0.14",
    "eva-icons": "1.1.2",
    "firebase": "7.7.0",
    "google-protobuf": "3.11.2",
    "grpc-web": "1.0.7",
    "ngx-auto-unsubscribe": "3.0.1",
    "prismjs": "1.19.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.4",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.0-rc.10",
    "@angular/cli": "9.0.0-rc.10",
    "@angular/compiler-cli": "9.0.0-rc.11",
    "@angular/language-service": "9.0.0-rc.11",
    "@compodoc/compodoc": "1.1.11",
    "@fortawesome/fontawesome-free": "5.12.0",
    "@nrwl/angular": "8.12.0-beta.4",
    "@nrwl/cypress": "8.12.0-beta.4",
    "@nrwl/jest": "8.12.0-beta.4",
    "@nrwl/nest": "8.12.0-beta.4",
    "@nrwl/node": "8.12.0-beta.4",
    "@nrwl/workspace": "8.12.0-beta.4",
    "@schematics/angular": "9.0.0-rc.10",
    "@scullyio/init": "0.0.18",
    "@scullyio/scully": "0.0.62",
    "@types/google-protobuf": "3.7.2",
    "@types/jest": "24.9.1",
    "@types/node": "13.5.0",
    "codecov": "3.6.2",
    "codelyzer": "5.2.1",
    "commitizen": "4.0.3",
    "cypress": "3.8.3",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "jest": "25.1.0",
    "jest-preset-angular": "7.1.1",
    "prettier": "1.19.1",
    "rimraf": "3.0.0",
    "standard-version": "7.1.0",
    "ts-jest": "25.0.0",
    "ts-node": "8.6.2",
    "tslint": "6.0.0",
    "typescript": "3.7.5"
  },
  "optionalDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "cz-conventional-changelog": "3.0.2",
    "firebase-tools": "7.12.1",
    "fuzzy": "0.1.3",
    "husky": "4.2.1",
    "inquirer": "7.0.4",
    "inquirer-autocomplete-prompt": "1.0.1",
    "lint-staged": "10.0.2",
    "lite-server": "2.5.4",
    "ngx-semantic-version": "2.0.0-beta.4",
    "webpack-bundle-analyzer": "3.6.0"
  }
}

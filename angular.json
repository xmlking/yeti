{
  "version": 2,
  "cli": {
    "packageManager": "yarn",
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "webapp",
  "generators": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@schematics/angular:component": {
      "style": "scss",
      "prefix": "yeti",
      "changeDetection": "OnPush"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "accounts": "libs/accounts",
    "admin": "libs/admin",
    "api": "apps/api",
    "blog": "libs/blog",
    "core": "libs/core",
    "dashboard": "libs/dashboard",
    "gen-grpcweb": "libs/gen/grpcweb",
    "gen-grpcweb2": "libs/gen/grpcweb2",
    "gen-nest": "libs/gen/nest",
    "gen-node": "libs/gen/node",
    "goapi": "apps/goapi",
    "home": "libs/home",
    "not-found": "libs/not-found",
    "shared": "libs/shared",
    "webapp": "apps/webapp",
    "workspace": "."
  }
}
